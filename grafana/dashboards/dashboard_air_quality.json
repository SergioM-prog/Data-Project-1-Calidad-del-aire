        {
          "datasource": { "type": "grafana-postgresql-datasource", "uid": "PostgreSQL" },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  (fecha_hora_alerta AT TIME ZONE 'Europe/Madrid')::timestamp as fecha_hora_alerta,\n  nombre_estacion,\n  ciudad,\n  CASE WHEN alerta_no2 THEN CONCAT('NO2: ', ROUND(valor_no2::numeric, 1), ' (lim: ', ROUND(limite_no2::numeric, 1), ')') ELSE NULL END as alerta_no2,\n  CASE WHEN alerta_pm10 THEN CONCAT('PM10: ', ROUND(valor_pm10::numeric, 1), ' (lim: ', ROUND(limite_pm10::numeric, 1), ')') ELSE NULL END as alerta_pm10,\n  CASE WHEN alerta_pm25 THEN CONCAT('PM2.5: ', ROUND(valor_pm25::numeric, 1), ' (lim: ', ROUND(limite_pm25::numeric, 1), ')') ELSE NULL END as alerta_pm25,\n  CASE WHEN alerta_so2 THEN CONCAT('SO2: ', ROUND(valor_so2::numeric, 1), ' (lim: ', ROUND(limite_so2::numeric, 1), ')') ELSE NULL END as alerta_so2,\n  CASE WHEN alerta_o3 THEN CONCAT('O3: ', ROUND(valor_o3::numeric, 1), ' (lim: ', ROUND(limite_o3::numeric, 1), ')') ELSE NULL END as alerta_o3,\n  CASE WHEN alerta_co THEN CONCAT('CO: ', ROUND(valor_co::numeric, 1), ' (lim: ', ROUND(limite_co::numeric, 1), ')') ELSE NULL END as alerta_co\nFROM marts.fct_alertas_actuales_contaminacion\nORDER BY fecha_hora_alerta DESC\nLIMIT 50;",
          "refId": "A"
        }
      ],
      "title": "Alertas Activas (Detalle)",
      "type": "table"
